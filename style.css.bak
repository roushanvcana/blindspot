.item {
     display: block;
     animation: fade_in_show 10s;
}

@keyframes fade_in_show {
     0% {
          opacity: 0;
          transform: scale(0)
     }
     50% {
		opacity: 1;
		transform: scale(1)
   }
     100% {
          opacity: 1;
          transform: scale(1)
     }
}
.d-none_show {
	display: none;
}

